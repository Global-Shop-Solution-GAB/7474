V.Local.fDiscount.Set(V.Passed.000026)
V.Local.fTax.Set(V.Passed.000109)
V.Local.fQty.Set(V.Passed.000021)
F.Intrinsic.Math.Sub(1,V.Local.fDiscount,V.Local.fDiscount)
F.Intrinsic.Math.Mult(V.Local.fCost2,V.Local.fDiscount,V.Local.fAmtS)
F.Intrinsic.Math.Mult(V.Local.fAmtS,V.Local.fQty,V.Local.fAmtS)
F.Intrinsic.Math.Mult(V.Local.fTax,V.Local.fDiscount,V.Local.fTax)
F.Intrinsic.Math.Mult(V.Local.fTax,V.Local.fQty,V.Local.fTax)
F.Intrinsic.Math.Add(V.Local.fAmtS,V.Local.fTax,V.Local.fAmtS)

'Modify by Deni Y (01/03/2020) Create compare value for After with Cost without Tax * Qty, 
F.Intrinsic.Math.Mult(V.Local.fCost2,V.Local.fQty,V.Local.fAmtS2)

'Get the PO reference file from GSS temp folder
F.Intrinsic.String.Format(V.Ambient.Date,"YYYYMMDD",V.Local.sDate)
F.Intrinsic.String.Concat(V.System.Temp,"\GSS\INA_",V.Caller.User,"_",V.Local.sDate,"_PO.txt",V.Local.sFileName)
F.Intrinsic.File.File2String(V.Local.sFileName,V.Local.sFile)
F.Intrinsic.String.Split(V.Local.sFile,V.Ambient.NewLine,V.Local.sFile)

'Go through every line that matches and update the record
V.Local.sFileRW.Set("")
V.Local.bExist.Set(False)
F.Intrinsic.Control.For(V.Local.i1,0,V.Local.sFile.UBound,1)
	F.Intrinsic.String.Split(V.Local.sFile(V.Local.i1).Trim,"*!*",V.Local.sTemp)
	F.Intrinsic.String.IsInString(V.Local.sTemp(0).Trim,V.Local.sPOLine.Trim,True,V.Local.bCheck)
	F.Intrinsic.Control.If(V.Local.bCheck,=,True)
